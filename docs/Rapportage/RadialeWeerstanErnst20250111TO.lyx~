\begin{equation}
\boxed{\Omega_{r\to0}=-\frac{Q}{2\pi}\left[\ln\frac{r}{D}+i\theta\right],\text{voor}\;z\to b+iD}\label{eq:limit_omega_at_extraction_point}
\end{equation}

\end_inset


