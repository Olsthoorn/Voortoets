\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{dx}{dt} & =\epsilon\frac{N}{D}x\\
\frac{dx}{x} & =\epsilon\frac{N}{D}dt\\
\ln x & =\epsilon\frac{N}{D}\left(t-t_{0}\right)+C\rightarrow C=\ln x_{0}\\
\ln\frac{x}{x_{0}} & =\epsilon\frac{N}{D}\left(t-t_{0}\right)
\end{align*}

