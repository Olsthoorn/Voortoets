\begin_layout Standard
Wat is de potentiaal op grote afstand van de onttrekking (asymptoot)?
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Omega(z\rightarrow\infty)=\frac{2Q}{\pi}\,\ln\left(i\sinh\left(\frac{\pi z}{2D}\right)\right)-\frac{Q}{D}z
\]

\end_inset


\end_layout

\begin_layout Standard
dus,
 als boven met 
\begin_inset Formula $\ln i=i\pi/2$
\end_inset

 en 
\begin_inset Formula $\sinh w\approx\frac{1}{2}e^{w}$
\end_inset

 voor grote 
\begin_inset Formula $e$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Omega\left(z\rightarrow\infty\right) & =iQ+\frac{2Q}{\pi}\ln\left(\frac{1}{2}\exp\left(\frac{\pi z}{2D}\right)\right)-\frac{Q}{D}z\\
 & \approx\frac{2Q}{\pi}\frac{\pi z}{2D}-\frac{2Q}{\pi}\ln2-\frac{Q}{D}z\\
 & =-\frac{2Q}{\pi}\ln2
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Omega\left(z\rightarrow\infty\right)=-\frac{2Q}{\pi}\ln2
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\boxed{\phi\left(z\to\infty\right)=-\frac{2Q}{\pi k}\ln2}\label{eq:dphi-aymptoot-contractie}
\end{equation}

\end_inset


\end_layout

