\begin_inset Formula 
\[
\Omega(z)=\frac{2Q}{\pi}\,\ln\left(\sin\left(\frac{i\pi}{2D}z\right)\right)-\frac{Q}{D}z\ +\frac{2Q}{\pi}\ln2;\qquad x\ge0,\ 0>y>-D
\]

\end_inset


\end_layout

\begin_layout Standard
Daar 
\begin_inset Formula $\sin iw=i\sinh iw$
\end_inset

 en voor kleine 
\begin_inset Formula $w$
\end_inset

 geldt 
\begin_inset Formula $\sinh iw\approx e^{iw}$
\end_inset

 wordt kleine 
\begin_inset Formula $z$
\end_inset

 wordt 
\begin_inset Formula $\sinh\left(\frac{\pi z}{2D}\right)\approx\left(\frac{\pi z}{2D}\right)$
\end_inset

.
 De twee term gaat naar nul.
 Voor 
\begin_inset Formula $z$
\end_inset

 kan je schrijven 
\begin_inset Formula $z=re^{i\theta}$
\end_inset

 Dus krijgen we
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Omega(z)\approx\frac{2Q}{\pi}\left(\ln i+\ln\left(\frac{\pi r}{2D}e^{i\theta}\right)+\ln2\right)
\]

\end_inset


\end_layout

\begin_layout Standard
met 
\begin_inset Formula $\ln i=\ln\left(1e^{i\frac{\pi}{2}}\right)=0+i\frac{\pi}{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Omega\left(z\right) & =\frac{2Q}{\pi}\left(i\frac{\pi}{2}+\ln\left(\frac{\pi r}{2D}\right)+i\theta+\ln2\right)\\
 & =\frac{2Q}{\pi}\left(\ln\left(\frac{\pi r}{D}\right)+i\left(\frac{\pi}{2}+\theta\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Met 
\begin_inset Formula $-\frac{\pi}{2}\le\theta\le0$
\end_inset

 in de doorsnede.
 Voor de stijghoogte 
\begin_inset Formula $\phi=\frac{\Phi}{k}$
\end_inset

voor kleine 
\begin_inset Formula $r$
\end_inset

 ten opzichte van 0 verderop in de aquifer,
 geldt
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\phi\left(r\right)=\frac{2Q}{\pi k}\ln\left(\frac{\pi r}{D}\right)
\]

\end_inset


\end_layout

